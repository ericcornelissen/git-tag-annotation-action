{
  "name": "git-tag-annotation-action",
  "version": "1.1.4",
  "private": true,
  "description": "Github Action for obtaining the annotation of a git tag",
  "homepage": "https://github.com/ericcornelissen/git-tag-annotation-action#readme",
  "license": "MIT",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf .temp/ _reports/",
    "coverage": "npm run test -- --coverage --coverageDirectory _reports/coverage",
    "format": "prettier --write ./**/*.{js,json,md,yml} !lib/* --ignore-path .gitignore",
    "lint": "prettier --check ./**/*.{js,json,md,yml} !lib/* --ignore-path .gitignore",
    "prepare": "is-ci || husky install script/hooks",
    "test": "jest",
    "test:mutation": "stryker run stryker.config.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericcornelissen/git-tag-annotation-action.git"
  },
  "bugs": {
    "url": "https://github.com/ericcornelissen/git-tag-annotation-action/issues"
  },
  "author": {
    "name": "Eric Cornelissen",
    "email": "ericornelissen@gmail.com",
    "url": "https://ericcornelissen.dev/"
  },
  "keywords": [
    "git",
    "tag",
    "annotation",
    "git-actions",
    "actions"
  ],
  "dependencies": {
    "@actions/core": "1.6.0",
    "shescape": "1.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@stryker-mutator/core": "5.6.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rollup": "2.66.0",
    "rollup-plugin-terser": "7.0.2"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": "<7.0.0"
  }
}
