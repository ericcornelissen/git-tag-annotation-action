    "annotation",
    "git-actions",
    "actions"
  ],
  "dependencies": {
    "@actions/core": "1.5.0",
    "shescape": "1.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@stryker-mutator/core": "5.4.1",
    "jest": "27.2.4",
    "rollup": "2.56.3",
